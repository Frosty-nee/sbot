#!/usr/bin/env python3

from bot import Bot
import code_eval
import eve
import utils

def main():
	bot = Bot({
		'calc': utils.calc,
		'roll': utils.roll,
		'time': utils.timezones,

		'pc': eve.price_check,
		'price': eve.price_check,
		'jumps': eve.jumps,
		'ly': eve.lightyears,

		'js': code_eval.nodejs,
		'ruby': code_eval.irb,
		'py2': code_eval.python2,
		'py3': code_eval.python3,
	})
	bot.connect()
	bot.run_forever()

if __name__ == '__main__':
	main()
